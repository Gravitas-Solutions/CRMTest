{
  "name": "crm_ui_tests",
  "version": "1.0.0",
  "description": "runs ui tests on crm client-side",
  "main": "index.js",
  "dependencies": {
    "package.json": "^2.0.1",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "cypress": "8.3.0",
    "eslint": "6.0.1",
    "react-scripts": "3.3.0",
    "tsc": "2.0.3"
  },
  "scripts": {
    "cypress:info": "cypress info",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "lint": "eslint examples",
    "lint:json": "eslint --ext .json integration",
    "start": "echo Call npm start from each example subfolder!",
    "stop-only": "stop-only --folder examples --skip node_modules --skip dist --skip .cache --skip build",
    "test:ci": "node ./test-examples",
    "test:ci:brave": "node ./test-examples --brave",
    "test:ci:chrome": "node ./test-examples --chrome",
    "test:ci:chrome:headless": "node ./test-examples --chrome --headless",
    "test:ci:firefox": "node ./test-examples --firefox",
    "test:ci:windows": "node ./test-examples --windows",
    "test:ci:windows:record": "node ./test-examples --windows --record",
    "build:ci": "react-scripts build",
    "types": "tsc --noEmit",
    "test:unit:ci": "react-scripts test --watchAll false --ci --runInBand",
    "warn-only": "stop-only --warn --folder examples --skip node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gravitas-Solutions/CRMTest.git"
  },
  "author": "greyhornet jey.mashjm2@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gravitas-Solutions/CRMTest/issues"
  },
  "homepage": "https://github.com/Gravitas-Solutions/CRMTest#readme"
}
